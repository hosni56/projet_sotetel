<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Accueil_agent commercial"
        android:id="@+id/textViewac"
        android:layout_gravity="center_horizontal"
        android:textAlignment="center"
        android:textColor="#f435ae" />
    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="GESTION PROJET"
        android:id="@+id/buttonGP"
        android:layout_gravity="center_horizontal"
        android:onClick="buttonGP"
        android:background="#ff9900"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="137dp" />
    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Accueil employe"
        android:id="@+id/buttonVL1"
        android:layout_gravity="center_horizontal"
        android:background="#dafb419e"
        android:onClick="buttonVL1"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="SE DECONNECTER"
        android:id="@+id/buttonD3"
        android:layout_gravity="center_horizontal"
        android:background="#9a5e9c"
        android:onClick="buttonD3"
        android:layout_below="@+id/buttonVL1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="58dp" />



</RelativeLayout>